<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="api">
	<select id="selectTest" resultType="com.hostate.api.vo.TestTableVO">
		select *
		from testtable
	</select>
	
	<update id="pwEncryptuUpdate" parameterType="hashmap">
		update tb_user_info
		set  user_pw = #{user_pw}
		where user_id = #{user_id}
	</update>
</mapper>
